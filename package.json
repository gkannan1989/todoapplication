  "main": "src/index.js",
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@material-ui/styles": "latest",
    "enzyme": "latest",
    "enzyme-adapter-react-16": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "latest"
  }, 
  "devDependencies": {},
  "scripts": {
